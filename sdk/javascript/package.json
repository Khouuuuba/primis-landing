{
  "name": "@primis/sdk",
  "version": "0.1.0",
  "description": "Official JavaScript/TypeScript SDK for Primis Protocol - AI Compute Platform",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "primis",
    "ai",
    "gpu",
    "compute",
    "inference",
    "serverless",
    "sdk"
  ],
  "author": "Primis Protocol",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/primis-protocol/sdk-js"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
